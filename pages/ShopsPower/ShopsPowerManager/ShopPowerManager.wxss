/* pages/ShopsPower/ShopsPowerManager/ShopPowerManager.wxss */

.account-item {
  background: #fff;
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 100%;
  height: 60px;
  border-bottom: 1px solid #f5f5f5;
  padding: 0 10px;
}
.leftitem {
  width: 60%;
  height: 100%;

}
.userphoneview {
  padding-top: 10px;
}
.userjobview {
  padding-top: 5px;
  color: #ccc;
}
.stateItem {
  width: calc(40% - 10px);
  padding-right: 10px;
  color: #ccc;
  text-align: right;
}
.bottomview{
  height: 50px;
  bottom: 0px;
  z-index: 999;
  position:fixed;
  left:0;
  right:0;
  display: flex;
  flex-direction: row;
}
.jobmanagerview {
  color: #fff;
  background: #666;
  width: 50%;
  text-align: center;
  line-height: 50px;
}
.addAccountview {
  color: #fff;
  background: #ff4444;
  width: 50%;
  text-align: center;
  line-height: 50px;
}