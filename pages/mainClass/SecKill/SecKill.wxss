.choosetishi {
  width: 100%;
  height: 30px;
  display: flex;
  flex-direction: row;
  align-items: center;
  background: #fff;
}
.tishipic {
  width: 15px;
  height: 15px;
  margin-left: 15px;
}
.tishiview {
  padding-left: 10px;
  color: #666;
  width: calc(100% - 50px);
