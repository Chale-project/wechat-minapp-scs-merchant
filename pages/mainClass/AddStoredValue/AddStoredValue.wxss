/* pages/mainClass/AddStoredValue/AddStoredValue.wxss */
.layout {
  display: flex;
  flex-direction: column;
}

.couponLayout {
  width: calc(100% - 30px);
  height: 55px;
  margin-left: 15px;
  display: flex;
  justify-content: space-between;
  align-items: center;
  background-color: #fff;
  border-radius: 0px;
  border: 1px solid #f1f1f1;
  position: absolute;
  top: 15px;
  left: 0px;
}

.layout_outside {
  position: relative;
  height: 80px;
}

.couponImage {
  height: 36px;
  margin-left: 10px;
  width: 54px;
}

.couponDelete {
  height: 25px;
  width: 25px;
  position: absolute;
  top: 4px;
  right:8px;
}


.couponDetail{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: right;
}