/* pages/mainClass/ShopSendMoneySet/ShopSendMoneySet.wxss */
.sendcontent {
  background: #fff;
  display: flex;
  align-items: center;
  flex-direction: column;
  padding: 20px;
}
.firview {
  display: flex;
  align-items: center;
  flex-direction: row;
  width: 100%;
}
.editInput {
  padding-left: 10px;
  border-bottom: 1px solid #666;
}
.secview {
  background: #f5f5f5;
  margin-top: 20px;
  width: 100%;
  height: 80px;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.onetextview {
  text-align: right;
  width: 80px;
}
.twotextview {
  text-align: left;
  width: 80px;
}
.twoeditInput {
  width: calc(100% - 170px);
  height: 40px;
  background: #fff; 
  text-align: center;
}
.complet {
  margin: 50px;
  font-size: 14px;
  height: 40px;
  background: #ff4444;
  color: #ffffff;
  border-radius: 5px;
  display: flex;
  align-items: center;
  justify-content: center;
}
.complet:active {
  opacity: .8;
}