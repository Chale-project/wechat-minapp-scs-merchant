/* pages/mainClass/SkillDataShow/SkillDataShow.wxss */

.secondView {
  height: 70px;
  background: #fff;
}
.secondView::after {
  content: '';
  display: block;
  clear: both;
}
.student-item {
  box-sizing: content-box;
  width: 33.3%;
  float: left;
}
.studentName {
  text-align:center;
  height: 60px;
  display: flex;
  align-items: center;
  flex-direction: column;
  justify-content: center;
}
.studentName text {
  font-weight: bold;
  color: #333;
  font-size: 13px;
}
.studentName view {
  margin-top: 8px;
  color: #000;
  font-size: 13px;
}
.sale-item {
  background: #fff;
  width: 100%;
  height: 50px;
  display: flex;
  align-items: center;
  flex-direction: row;
  padding: 0 10px;
  border-bottom: 1px solid #f5f5f5;
}
.iconimage{
  width: 30px;
  height: 30px;
  border-radius: 15px;
  background: #f5f5f5;
}
.nameview {
  width: 90px;
  padding-left: 10px;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  font-size: 12px;
}
.rightview {
  width: calc(100% - 120px);
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  text-align: right;
  color: #666;
  font-size: 12px;
}
.rightview text{
  color: #ff4444;
}

