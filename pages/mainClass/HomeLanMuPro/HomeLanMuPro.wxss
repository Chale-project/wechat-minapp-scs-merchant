/* pages/mainClass/HomeLanMuPro/HomeLanMuPro.wxss */
.choosePro {
  background: #ffffff;
  height: 40px;
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-bottom: 1px solid #f5f5f5;
  padding: 0 20px;
}

.topcater-itemstore {
  background:#ffffff;
  box-sizing: content-box;
  border-bottom: 1px solid #f5f5f5;
  display: flex;
  justify-content: center;
  flex-direction: column;
  float:left;
  width:calc(50% - 20px);
  padding: 10px;
  align-items:center;
  height: 170px;
}
.topcloseview {
  width: 100%;
  height: 20px;
  display: flex;
  flex-direction: row;
}
.closeview {
  width: 18px;
  height: 18px;
  border: 1px solid #666;
  border-radius: 10px;
  text-align: center;
}
.userIconImg {
  width: 100px;
  height: 100px;
}
.goodstitleview {
  width: 100%;
  margin-top: 5px;
  text-overflow: ellipsis;
  overflow: hidden;
  -webkit-box-orient:vertical;
  -webkit-line-clamp:2;
  text-align: center;
}


