/**index.wxss**/

.container {
  background: #f5f5f5;
} 

.usericonimage {
  width: 90px;
  height: 90px;
  margin-bottom: 50px;
}

.loginIphone {
  background:#fff;
  width: 320px;
  height: 46px;
  border-radius: 23px;
  display: flex;
  align-items: center;
}
.userPersonimage {
  width: 26px;
  height: 26px;
  margin-left:10px;
}
input {
  padding-left: 10px;
  background: #fff;
  width: calc(100% - 46px);
  min-height: 46px;
  height: 46px;
  display: flex;
  align-items: center;
}
.loginPassword {
background:#fff;
width:320px;
height:46px;
border-radius:23px;
margin-top:20px;
justify-content:space-between;
display:flex;
align-items:center;
}
.userPasswordimage {
  width:26px;
  height:26px;
  margin-left:10px;
}
.placeholdertwo {
  width:calc(100% - 82px);
  background:#fff;
}
.signhidepassword {
  width:26px;
  height:26px;
  margin-right:10px;
}
.placeholder {
  color: #9a9a9a;
}
.confirm-btn {
  font-size:13pt;
  height:46px;
  border-radius:23px;
  width:300px;
  display:flex;
  align-items:center;
  justify-content:center;
  color:#fff;
  margin-top:50px;
}
.confirm-btn:active {
  opacity: .8;
}
.remmberClass {
  width: 300px;
  height: 20px;
  justify-content:space-between;
  display:flex;
  margin-top:20px;
}
.rember-btn {
  font-size: 12px;
  color: #666;
}
.forget-btn {
  font-size: 12px;
  color: #666;
}
